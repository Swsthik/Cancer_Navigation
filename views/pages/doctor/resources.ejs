    <style>
      :root {
        --primary-blue: #2a5c82;
        --secondary-blue: #5c9baf;
        --accent-teal: #4ecdc4;
        --high-risk: #dc3545;
        --medium-risk: #ffc107;
        --low-risk: #28a745;
      }

      body {
        background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
        min-height: 100vh;
      }

      .medical-header {
        background: var(--primary-blue);
        padding: 1rem;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
      }

      .dashboard-container {
        background: rgba(255, 255, 255, 0.95);
        box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s ease;
      }

      .btn-medical {
        background: var(--accent-teal);
        color: white;
        padding: 12px 30px;
        transition: all 0.3s ease;
      }

      .btn-medical:hover {
        background: var(--secondary-blue);
        transform: translateY(-2px);
      }

      .btn-outline-medical {
        color: var(--accent-teal);
        border: 1px solid var(--accent-teal);
        transition: all 0.3s ease;
      }

      .btn-outline-medical:hover {
        background: var(--accent-teal);
        color: white;
      }

      .card-medical {
        border-radius: 15px;
        box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
        transition: all 0.3s ease;
      }

      .card-medical:hover {
        transform: translateY(-5px);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
      }

      .sidebar {
        background: var(--primary-blue);
        color: white;
      }

      .nav-link {
        color: rgba(255, 255, 255, 0.8);
        transition: all 0.3s ease;
        padding: 0.8rem 1rem;
        border-radius: 10px;
        margin-bottom: 0.5rem;
      }

      .nav-link:hover, .nav-link.active {
        background: rgba(255, 255, 255, 0.1);
        color: white;
      }

      .nav-link i {
        margin-right: 10px;
      }

      .decorative-wave {
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 150px;
        background: url('data:image/svg+xml;utf8,<svg viewBox="0 0 1440 320" xmlns="http://www.w3.org/2000/svg"><path fill="%232A5C82" fill-opacity="0.1" d="M0,160L48,170.7C96,181,192,203,288,186.7C384,171,480,117,576,101.3C672,85,768,107,864,138.7C960,171,1056,213,1152,208C1248,203,1344,149,1392,122.7L1440,96L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>');
      }

      .resource-card {
        height: 100%;
        border-radius: 15px;
        overflow: hidden;
        transition: all 0.3s ease;
      }

      .resource-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
      }

      .resource-card .card-img-top {
        height: 160px;
        object-fit: cover;
      }

      .resource-card .card-body {
        padding: 1.5rem;
      }

      .resource-badge {
        position: absolute;
        top: 10px;
        right: 10px;
        font-size: 0.7rem;
        padding: 0.3rem 0.6rem;
        border-radius: 20px;
      }

      .resource-badge-new {
        background-color: var(--accent-teal);
        color: white;
      }

      .resource-badge-popular {
        background-color: var(--primary-blue);
        color: white;
      }

      .resource-badge-updated {
        background-color: var(--medium-risk);
        color: black;
      }

      .resource-category {
        font-size: 0.8rem;
        color: var(--secondary-blue);
        margin-bottom: 0.5rem;
      }

      .resource-meta {
        font-size: 0.8rem;
        color: #6c757d;
      }

      .search-container {
        position: relative;
      }

      .search-container i {
        position: absolute;
        left: 15px;
        top: 50%;
        transform: translateY(-50%);
        color: #6c757d;
      }

      .search-input {
        padding-left: 40px;
        border-radius: 20px;
      }

      .category-filter {
        background-color: #f8f9fa;
        border-radius: 10px;
        padding: 20px;
        margin-bottom: 20px;
      }

      .category-item {
        display: flex;
        align-items: center;
        padding: 10px;
        border-radius: 10px;
        transition: all 0.3s ease;
        cursor: pointer;
      }

      .category-item:hover, .category-item.active {
        background-color: rgba(76, 205, 196, 0.1);
      }

      .category-item i {
        font-size: 1.2rem;
        margin-right: 10px;
        color: var(--accent-teal);
      }

      .category-item.active i {
        color: var(--primary-blue);
      }

      .category-count {
        margin-left: auto;
        background-color: #e9ecef;
        color: #6c757d;
        font-size: 0.8rem;
        padding: 0.2rem 0.5rem;
        border-radius: 10px;
      }

      .nav-pills .nav-link {
        color: #6c757d;
        border-radius: 0.5rem;
      }

      .nav-pills .nav-link.active {
        background-color: var(--accent-teal);
        color: white;
      }

      .resource-detail-header {
        background-color: #f8f9fa;
        border-radius: 15px;
        padding: 30px;
        margin-bottom: 30px;
      }

      .resource-detail-meta {
        display: flex;
        align-items: center;
        margin-bottom: 15px;
      }

      .resource-detail-meta-item {
        display: flex;
        align-items: center;
        margin-right: 20px;
        color: #6c757d;
        font-size: 0.9rem;
      }

      .resource-detail-meta-item i {
        margin-right: 5px;
        color: var(--accent-teal);
      }

      .resource-detail-tags {
        margin-top: 15px;
      }

      .resource-tag {
        background-color: rgba(76, 205, 196, 0.1);
        color: var(--accent-teal);
        font-size: 0.8rem;
        padding: 0.3rem 0.6rem;
        border-radius: 20px;
        margin-right: 5px;
        margin-bottom: 5px;
        display: inline-block;
      }

      .resource-action {
        display: flex;
        align-items: center;
        padding: 10px 15px;
        border-radius: 10px;
        transition: all 0.3s ease;
        cursor: pointer;
        margin-bottom: 10px;
      }

      .resource-action:hover {
        background-color: rgba(76, 205, 196, 0.1);
      }

      .resource-action i {
        font-size: 1.2rem;
        margin-right: 10px;
        color: var(--accent-teal);
      }

      .bookmark-btn {
        color: #6c757d;
        transition: all 0.3s ease;
      }

      .bookmark-btn:hover, .bookmark-btn.active {
        color: var(--accent-teal);
      }

      .bookmark-btn.active i {
        font-weight: 900;
      }

      .related-resource {
        display: flex;
        align-items: center;
        padding: 15px;
        border-radius: 10px;
        transition: all 0.3s ease;
        margin-bottom: 10px;
        background-color: #f8f9fa;
      }

      .related-resource:hover {
        background-color: rgba(76, 205, 196, 0.1);
      }

      .related-resource-icon {
        width: 40px;
        height: 40px;
        border-radius: 10px;
        background-color: var(--accent-teal);
        color: white;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-right: 15px;
      }

      .related-resource-content {
        flex: 1;
      }

      .related-resource-title {
        font-weight: 600;
        margin-bottom: 5px;
      }

      .related-resource-meta {
        font-size: 0.8rem;
        color: #6c757d;
      }
    </style>
  </head>
